{
    "cpu": "default",
    "smp_passthrough": false,
    "cpu_mitigations": false,
    "topology_enabled": false,
    "smp_cpus": 4,
    "smp_sockets": 1,
    "smp_cores": 1,
    "smp_threads": 1,
    "architecture": "Architecture: x86_64",
    "qemu_executable": "qemu-system-x86_64",
    "custom_executable": "",
    "drives": [
        {
            "id": "disk0",
            "file": "hdd/win98.qcow2",
            "if": "scsi",
            "media": "disk",
            "format": "qcow2"
        },
        {
            "id": "disk1",
            "file": "extras/disk.img",
            "if": "scsi",
            "media": "disk",
            "format": "raw"
        },
        {
            "id": "disk2",
            "file": "cdrom/win98qi_v0.9.6_ALL.iso",
            "if": "none",
            "media": "cdrom"
        },
        {
            "id": "disk3",
            "file": "cdrom/softgpu-0.6.2024.40.iso",
            "if": "none",
            "media": "cdrom"
        },
        {
            "id": "disk4",
            "file": "cdrom/wrappers.iso",
            "if": "none",
            "media": "cdrom"
        },
        {
            "id": "disk5",
            "file": "cdrom/mesa9x-24.1.5.114-driver-win98.iso",
            "if": "none",
            "media": "cdrom"
        },
        {
            "id": "disk6",
            "file": "cdrom/Security9_enu_15.iso",
            "if": "none",
            "media": "cdrom"
        },
        {
            "id": "disk7",
            "file": "cdrom/vmaddons.iso",
            "if": "none",
            "media": "cdrom"
        }
    ],
    "floppies": [
        {
            "unit": 0,
            "file": "floppy/disks/abd4/fd4.ima"
        }
    ],
    "qemu_args": "-drive file=hdd/win98.qcow2,id=disk0,if=scsi,format=qcow2 \\\n-drive file=extras/disk.img,id=disk1,if=scsi,format=raw \\\n-drive file=cdrom/win98qi_v0.9.6_ALL.iso,id=disk2,if=none \\\n-drive file=cdrom/softgpu-0.6.2024.40.iso,id=disk3,if=none \\\n-drive file=cdrom/wrappers.iso,id=disk4,if=none \\\n-drive file=cdrom/mesa9x-24.1.5.114-driver-win98.iso,id=disk5,if=none \\\n-drive file=cdrom/Security9_enu_15.iso,id=disk6,if=none \\\n-drive file=cdrom/vmaddons.iso,id=disk7,if=none",
    "scsi_controller_needed": true,
    "virtio_controller_needed": false,
    "kvm_acceleration": true,
    "machine_type": "q35",
    "memory_mb": 10244
}